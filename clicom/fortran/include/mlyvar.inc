C
C     *** INCLUDE FILE :MLYVAR.INC IS REQUIRED FOR PROGRAM MLYPUB.FOR
C
C     THE PROGRAM CONTROL VARIABLES
C
      INTEGER*4      STRTYRMO
      INTEGER*2      RELELEM(1000),NUMELEM,LINECNT,IPAGE,MAXLINE,
     +               STNCNT,ELMCNT,PRTCNT,NHDLINES
      CHARACTER*1    RTNFLG
      CHARACTER*8    STARTSTN,ENDSTN
      LOGICAL        PRTELM(MAXELEM),FIRSTCALL,HDFLAG
C
      COMMON /CNTRL/ STRTYRMO,RELELEM,NUMELEM,LINECNT,IPAGE,
     +               MAXLINE,STNCNT,ELMCNT,PRTCNT,STARTSTN,ENDSTN,
     +               PRTELM,FIRSTCALL,HDFLAG,NHDLINES,RTNFLG
C
C   VARIABLES HOLDING ALL OF THE OUTPUT TITLES AND LEGENDS AND THE 
C   OUTPUT FORMATS FOR USER-SPECIFIED ELEMENTS
C
      CHARACTER*32    TITLE(4)
      CHARACTER       MTL13*4,MTL14*30,MTL15*20,MTL16*28
      CHARACTER       MTL21*20,MTL22*8,MTL23*4
      CHARACTER*8     MTL24(4)
      CHARACTER*6     MTL25(4),PAGNAM
      CHARACTER*5     MTL26(3)
      CHARACTER*120   MFOOT(2)
      CHARACTER       DTL13*28,DTL14*8,DTL22*8,DTL24*8,DTL25*8
      CHARACTER       STL1*16,STL2*20,STL3*16
      CHARACTER*9     STL4,STL5,STL6
      CHARACTER       ETL1*7,ETL2*5,ETL3*16,ETL4*16
      CHARACTER*7     ETL5(3)
      LOGICAL         ESKIP(MAXELEM)
      CHARACTER*4     SMYFMT(MAXELEM),VALFMT(MAXELEM),SYSTYP
      CHARACTER*1     AVGTOT(MAXELEM)
      COMMON /PARAMS/ TITLE,MTL13,MTL14,MTL15,MTL16,MTL21,MTL22,MTL23
     +   ,MTL24,MTL25,MTL26,MFOOT,DTL13,DTL14,DTL22,DTL24,DTL25,STL1
     +   ,STL2,STL3,STL4,STL5,STL6,ETL1,ETL2,ETL3,ETL4,ETL5,PAGNAM
     +   ,SMYFMT,VALFMT,AVGTOT,SYSTYP,ESKIP
C
C    INPUT DATA, ELEMENT DEFINITION, STATION DEFINITION, AND PRINTER VARIABLES
C
      INTEGER*2     DDSID,IELEM,ELEM(MAXELEM),YEAR,MONTH
      REAL*4        VALUE(31)
      CHARACTER*1   FLAG1(31)
      CHARACTER*8   STNID
      CHARACTER*24  STNABRV,PRTNAM 
      CHARACTER*6   ELEMABRV(MAXELEM)
      CHARACTER*16  ELEMNAME(MAXELEM)
      CHARACTER*20  ELEMUNITS(MAXELEM)
      CHARACTER*22  MONYEAR
      CHARACTER*1   SETLRG(8),SETNRM(8),STRTUND(8),STPUND(8),
     +              SETSML(8),SETLJLPI(8),LJRESET(8),LINCHR(11) 
C
      COMMON /MISC/ DDSID,IELEM,ELEM,YEAR,MONTH,VALUE,FLAG1,STNID,
     +              STNABRV,PRTNAM,ELEMABRV,ELEMNAME,ELEMUNITS,
     +              MONYEAR,SETLRG,SETNRM,SETSML,STRTUND,STPUND,
     +              LINCHR,SETLJLPI,LJRESET
